<?xml version="1.0" encoding="utf-8"?>
<xs:schema targetNamespace="http://www.nerazvod.hz/data/clients"
    elementFormDefault="qualified"
    xmlns="http://tempuri.org/XMLSchema.xsd"
    xmlns:mstns="http://tempuri.org/XMLSchema.xsd"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
>
	<xs:elment name =" bank">
		<xs:complexType>
			<xs:sequence>
				<xs:element name = "Name">
					<xs:complexType>
						<xs:sequence>
							<xs:element name ="FirstName" type="xs:string"/>
							<xs:element name ="SecondName" type="xs:string"/>
							<xs:element name ="LastName" type="xs:string"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>

				<xs:element name ="phoneList">
					<xs:sequence>
						<xs:element name ="workPhone" type="xs:string"/>
						<xs:element name ="homePhone" type="xs:string"/>
						<xs:element name ="mobilePhone" type="xs:string"/>
					</xs:sequence>
				</xs:element>

				<xs:element name ="passportInfo">
					<xs:sequence>
						<xs:element name ="serial" type="xs:string"/>
						<xs:element name ="number" type="xs:string"/>
						<xs:element name ="policeName" type="xs:string"/>
						<xs:element name ="date" type="xs:string"/>
					</xs:sequence>
				</xs:element>

				<xs:element name="credit">
					<xs:sequence>
						<xs:element name ="date" type="xs:string"/>
						<xs:element name ="type" type="xs:string"/>
						<xs:element name ="sum" type="xs:uinteger"/>
						<xs:element name ="procent" type="xs:double"/>
						<xs:element name ="term" type="xs:string"/>
					</xs:sequence>
				</xs:element>

				<xs:element name="debit">
					<xs:sequense>
						<xs:element name ="number" type="xs:string"/>
						<xs:element name ="date" type="xs:string"/>
						<xs:element name ="type" type="xs:string"/>
						<xs:element name ="sum" type="xs:unsignedInt"/>
						<xs:element name ="procent" type="xs:decimal"/>
						<xs:element name ="term" type="xs:string"/>
					</xs:sequense>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:elment>
</xs:schema>
